<div class="page-header">
  <h1>
    Question lengths
    <a href="/question_lengths/new" class="btn btn-lg btn-success">New Question length</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Name</th>
        <th>Actions</th>
      </tr>

      <% @question_lengths.each do |question_length| %>
      <tr>
        <td><%= question_length.name %></td>
        <td>
          <a href="/question_lengths/<%= question_length.id %>" class="btn btn-primary">Show</a>
          <a href="/question_lengths/<%= question_length.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_question_length/<%= question_length.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
